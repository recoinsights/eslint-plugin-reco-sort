{
  "name": "@recoinsights/eslint-plugin-reco-sort",
  "version": "1.7.0",
  "description": "An ESLint plugin for sorting things",
  "author": "Re:Co Insights",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc --project tsconfig.prod.json && rollup -c",
    "eslint": "eslint 'src/**/*.{js,ts,tsx}' 'test/**/*.{js,ts,tsx}'",
    "eslint:fix": "npm run eslint -- --fix",
    "prettier": "prettier 'src/**/*.{js,ts,tsx}' 'test/**/*.{js,ts,tsx}'",
    "prettier:fix": "npm run prettier -- --fix"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "esbuild": "^0.14.11",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "prettier": "^2.7.1",
    "rollup": "^4.20.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.1.3"
  },
  "files": [
    "lib/"
  ]
}
